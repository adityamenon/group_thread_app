{
  "name": "deepstream.io-client-js",
  "version": "0.4.3",
  "description": "the javascript client for deepstream.io",
  "main": "src/client.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "node node_modules/jasmine-node/lib/jasmine-node/cli.js test-unit test-e2e --captureExceptions --forceexit",
    "watch": "node node_modules/jasmine-node/lib/jasmine-node/cli.js test-unit --captureExceptions --autotest --watch ./src",
    "e2e": "node node_modules/jasmine-node/lib/jasmine-node/cli.js test-e2e --captureExceptions",
    "unit": "node node_modules/jasmine-node/lib/jasmine-node/cli.js test-unit --captureExceptions"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deepstreamIO/deepstream.io-client-js.git"
  },
  "dependencies": {
    "engine.io-client": "1.6.8",
    "component-emitter": "1.1.2"
  },
  "browser": {
    "net": false
  },
  "devDependencies": {
    "jasmine-core": "~2.4.1",
    "grunt-browserify": "4.0.1",
    "derequire": "2.0.3",
    "grunt": "~0.4.5",
    "grunt-contrib-uglify": "0.11.1",
    "grunt-exec": "0.4.6",
    "browserify": "13.0.0",
    "proxyquire": "1.7.4",
    "jasmine-node": "1.14.5",
    "grunt-release": "0.13.0",
    "deepstream.io": "latest",
    "deepstream.io-msg-redis": "latest"
  },
  "author": "deepstreamHub GmbH i. Gr.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deepstreamIO/deepstream.io-client-js/issues"
  },
  "homepage": "http://deepstream.io"
}
